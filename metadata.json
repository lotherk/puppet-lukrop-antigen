{
  "name": "lukrop-antigen",
  "version": "0.1.2",
  "author": "lukrop",
  "summary": "Installs antigen to easily manage your zsh plugins, called bundles (e.g. oh-my-zsh).",
  "license": "Apache-2.0",
  "source": "https://github.com/lukrop/puppet-lukrop-antigen.git",
  "project_page": "https://github.com/lukrop/puppet-lukrop-antigen",
  "issues_url": "https://github.com/lukrop/puppet-lukrop-antigen/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.9.0"},
    {"name":"puppetlabs-vcsrepo","version_requirement":">= 1.3.1"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04",
        "15.04"
      ]
    }
  ]
}

