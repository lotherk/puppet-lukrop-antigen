source <%= @antigen_repo %>/antigen.zsh
antigen use <%= @library %>
antigen theme <%= @theme %>
<% unless @bundles.empty? -%>
antigen bundles <<EOBUNDLES
<% @bundles.flatten.each do |bundle| -%>
  <%= bundle %>
<% end -%>
EOBUNDLES
<% end %>
antigen apply

<% unless @update_prompt -%>DISABLE_UPDATE_PROMPT=true<% end %>
<% unless @auto_update -%>DISABLE_AUTO_UPDATE=true<% end %>

